{
    "name": "sara-portfolio",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://www.sara-art.uk/",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "yarn build && cp build/index.html build/404.html && cp ./CNAME build/CNAME",
        "deploy": "gh-pages -d build",
        "optimize-images": "node scripts/optimize-images.js",
        "generate-thumbnails": "node --max-old-space-size=4096 scripts/generate-thumbnails.js",
        "prebuild": "yarn generate-thumbnails"
    },
    "dependencies": {
        "@emailjs/browser": "^4.4.1",
        "bootstrap": "^5.2.3",
        "emailjs-com": "^3.2.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.7.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.0.7",
        "react-icons": "^4.1.0",
        "react-router-dom": "^6.6.2",
        "react-scripts": "^5.0.1",
        "react-transition-group": "^4.4.2",
        "sharp": "^0.34.1",
        "typewriter-effect": "^2.18.2",
        "web-vitals": "^3.1.1"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "gh-pages": "^6.3.0"
    }
}